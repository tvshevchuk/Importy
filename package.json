{
  "name": "importy",
  "version": "0.1.4",
  "description": "A CLI tool for analyzing JavaScript/TypeScript imports from libraries",
  "homepage": "https://tvshevchuk.github.io/Importy/",
  "bugs": {
    "url": "https://github.com/tvshevchuk/Importy/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tvshevchuk/Importy"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "importy": "./dist/index.js"
  },
  "author": "Taras Shevchuk",
  "license": "MIT",
  "packageManager": "pnpm@10.11.1",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "prebuild": "npm run check",
    "prepublishOnly": "npm run test && npm run build",
    "version": "npm run check:fix && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major",
    "release:check": "./scripts/release.sh --check",
    "release:manual": "./scripts/release.sh --manual",
    "prerelease": "npm run test && npm run check",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@babel/types": "7.28.6",
    "@biomejs/biome": "2.3.4",
    "@types/babel__traverse": "7.20.7",
    "@types/node": "22.19.3",
    "@types/picomatch": "4.0.2",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitepress": "1.6.4",
    "vitest": "4.0.17",
    "vue": "3.5.25"
  },
  "dependencies": {
    "@babel/parser": "7.28.5",
    "@babel/traverse": "7.28.6",
    "commander": "14.0.2",
    "picomatch": "4.0.3"
  },
  "keywords": [
    "cli",
    "import",
    "imports",
    "analyze",
    "analysis",
    "javascript",
    "typescript",
    "jsx",
    "tsx",
    "library",
    "dependencies",
    "dependency",
    "scanner",
    "ast",
    "babel",
    "codebase",
    "tool",
    "static-analysis",
    "code-analysis",
    "module",
    "modules",
    "es6",
    "esm",
    "commonjs",
    "react",
    "vue",
    "angular",
    "component",
    "components",
    "usage",
    "report",
    "audit",
    "bundle",
    "bundler",
    "webpack",
    "vite",
    "rollup",
    "tree-shaking",
    "dead-code",
    "refactor",
    "migration"
  ]
}
