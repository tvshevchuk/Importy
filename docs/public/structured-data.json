    {
      "@type": "FAQPage",
      "@id": "https://tvshevchuk.github.io/Importy/#faq",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "What is Importy?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Importy is a powerful CLI tool for analyzing JavaScript/TypeScript imports from libraries. It helps developers identify which components, functions, and modules are actually being used in their codebase."
          }
        },
        {
          "@type": "Question",
          "name": "How do I install Importy?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "You can install Importy globally using npm: npm install -g importy. Requires Node.js version 18 or higher."
          }
        },
        {
          "@type": "Question",
          "name": "What file types does Importy support?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Importy supports JavaScript (.js), TypeScript (.ts), JSX (.jsx), and TSX (.tsx) files. It can analyze ES6 imports, CommonJS requires, and dynamic imports."
          }
        },
        {
          "@type": "Question",
          "name": "Can I use Importy in CI/CD pipelines?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes, Importy is designed for CI/CD integration. It provides JSON output, proper exit codes, and can be automated for dependency auditing and code quality checks."
          }
        },
        {
          "@type": "Question",
          "name": "Is Importy free to use?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes, Importy is completely free and open-source software released under the MIT License. You can use it for personal and commercial projects."
          }
        }
      ]
